{
  "name": "cybercore-charts",
  "version": "0.2.0",
  "type": "module",
  "description": "Zero-dependency cyberpunk SVG chart library with neon aesthetics and glitch effects",
  "main": "dist/cybercore-charts.js",
  "module": "dist/cybercore-charts.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cybercore-charts.esm.js",
      "require": "./dist/cybercore-charts.js",
      "default": "./dist/cybercore-charts.esm.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./styles": "./src/styles/cyber-charts.scss"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite build --watch",
    "dev:demo": "vite --config demo/vite.config.ts",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "vite build",
    "build:umd": "BUILD_UMD=true vite build",
    "build:umd:min": "BUILD_UMD_MIN=true vite build",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:demo": "vite build --config demo/vite.config.ts",
    "preview": "vite preview --config demo/vite.config.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "husky || true"
  },
  "keywords": [
    "charts",
    "svg",
    "cyberpunk",
    "data-visualization",
    "typescript",
    "zero-dependency",
    "neon",
    "glitch",
    "futuristic",
    "dark-theme",
    "bar-chart",
    "line-chart",
    "pie-chart",
    "radar-chart",
    "react",
    "react-charts",
    "react-components",
    "ember",
    "ember-charts",
    "ember-components",
    "glimmer",
    "octane"
  ],
  "author": "sebyx07",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebyx07/cybercore-charts.git"
  },
  "bugs": {
    "url": "https://github.com/sebyx07/cybercore-charts/issues"
  },
  "homepage": "https://sebyx07.github.io/cybercore-charts",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.3.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "sass": "^1.97.2",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@ember/object": ">=4.0.0",
    "@glimmer/component": ">=1.1.0",
    "@glimmer/tracking": ">=1.1.0",
    "ember-modifier": ">=4.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@glimmer/component": {
      "optional": true
    },
    "@glimmer/tracking": {
      "optional": true
    },
    "@ember/object": {
      "optional": true
    },
    "ember-modifier": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
